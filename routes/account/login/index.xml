<locales>
    <locale path="@routes/href">
        <en>/en/account/login/</en>
        <es>/es/cuenta/entra/</es>
        <it>/it/utente/entra/</it>
    </locale>
    <locale path="@routes/title">
        <en>Sign in</en>
        <es>Entra</es>
        <it>Entra</it>
    </locale>
    <locale path="@path/title">
        <en>Sign in</en>
        <es>Entra</es>
        <it>Entra</it>
    </locale>
    <locale path="@path/email/legend">
        <en>please enter your email to proceed</en>
        <es>Por favor ingresa tu correo eletronico para proceder</es>
        <it>Per favore immetti l'indirizzo di posta elettronica per procedere</it>
    </locale>
    <locale path="@path/email/placeholder">
        <en>Email</en>
        <es>Email</es>
        <it>Email</it>
    </locale>
    <locale path="@path/email/proceed">
        <en>PROCEED</en>
        <es>SEGUIR</es>
        <it>AVANTI</it>
    </locale>
    <locale path="@path/email/footer">
        <en>
            <p><span>Still not with us?</span> <a data-route="account/register/">Register here</a></p>
            <hr/>
            <p><span>Did you forgot your password?</span> <a data-route="account/reset/">Reset here</a></p>
        </en>
        <es>
            <p><span>Aun no tienes una cuenta?</span> <a data-route="account/register/">Creane una aquì</a></p>
            <hr/>
            <p><span>¿Has olvidado tu contraseña?</span> <a data-route="account/reset/">Cambiala aquì</a></p>
        </es>
        <it>
            <p><span>Non sei ancora dei nostri?</span> <a data-route="account/register/">Registrati qui</a></p>
            <hr/>
            <p><span>Hai dimenticato la password?</span> <a data-route="account/reset/">Cambiala qui</a></p>
        </it>
    </locale>
    <locale path="@path/password/legend">
        <en>please enter your passwrod to proceed</en>
        <es>Por favor ingresa la contraseña para proceder</es>
        <it>Per favore immetti la password per procedere</it>
    </locale>
    <locale path="@path/password/placeholder">
        <en>Password</en>
        <es>Contraseña</es>
        <it>Password</it>
    </locale>
    <locale path="@path/password/proceed">
        <en>PROCEED</en>
        <es>SEGUIR</es>
        <it>AVANTI</it>
    </locale>
</locales>
